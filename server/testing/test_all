export CHALMERS_DEPLOY_LEVEL="devel"
export ADHOC_RUNTIME_HOME=/Users/bernerus/work/proj/workspace/AdHoc
. $ADHOC_RUNTIME_HOME/.bashrc
cd $ADHOC_RUNTIME_HOME

